{
  "name": "cucumber-mink",
  "description": "Cucumber-js Mink - Step definitions library",
  "keywords": [
    "testing",
    "bdd",
    "cucumber",
    "cucumber-js",
    "gherkin",
    "tests",
    "webdriver",
    "phantomjs",
    "selenium"
  ],
  "version": "1.0.2",
  "license": "MIT",
  "author": {
    "name": "Arnaud Dezandee",
    "email": "dezandee.arnaud@gmail.com"
  },
  "homepage": "https://github.com/Adezandee/cucumber-mink",
  "repository": {
    "type": "git",
    "url": "git@github.com:Adezandee/cucumber-mink.git"
  },
  "bugs": {
    "url": "http://github.com/Adezandee/cucumber-mink/issues"
  },
  "main": "dist/mink.js",
  "bin": {
    "cucumber-mink": "./bin/cucumber-mink"
  },
  "scripts": {
    "lint": "eslint .",
    "compile": "babel -d dist/ src/",
    "start": "babel-node test/site/server.js",
    "coverage": "istanbul cover test/runner.js --root ./lib",
    "test": "babel-node test/runner.js",
    "prepublish": "rm -rf dist/ && npm run compile"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "async": "^1.5.0",
    "build-prototype": "^1.1.0",
    "chai": "^3.4.0",
    "cucumber": "^0.8.1",
    "debug": "^2.2.0",
    "lodash": "^3.10.1",
    "webdriverio": "^3.2.6",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "body-parser": "^1.14.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "express": "^4.13.3",
    "istanbul": "^0.4.0",
    "swig": "^1.4.2",
    "webdriver-manager": "^7.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true
    }
  }
}
